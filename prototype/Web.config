<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="index.html">
    <system.webserver>
      <caching>
        <profiles>
          <add varybyquerystring="*" location="Any"
            duration="00:01:00" policy="CacheForTimePeriod"
            extension=".html" />
          </profiles>
      </caching>
    </system.webserver>
  </location>
  <system.webServer>
    <staticContent>
      <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
    </staticContent>
  </system.webServer>
</configuration>
